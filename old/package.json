  "scripts": {
    "build": "run-s rebuild:electron pack build-release",
    "build-release": "node _scripts/build.js",
    "debug": "run-s rebuild:electron debug-runner",
    "debug-runner": "node _scripts/dev-runner.js --remote-debug",
    "dev": "run-s rebuild:electron dev-runner",
    "dev-runner": "node _scripts/dev-runner.js",
    "electron-builder-install": "electron-builder install-app-deps",
    "electron-rebuild": "electron-rebuild",
    "jest:coverage": "jest ./__tests__/utilities/ --collect-coverage",
    "jest:e2e": "jest ./__tests__/e2e/",
    "jest:unit": "jest ./__tests__/utilities/",
    "jest:watch": "jest ./__tests__/utilities/ --watch",
    "lint": "eslint --fix --ext .js,.ts,.vue ./",
    "pack": "run-p pack:main pack:renderer pack:workers",
    "pack:main": "webpack --mode=production --hide-modules --config _scripts/webpack.main.config.js",
    "pack:renderer": "webpack --mode=production --hide-modules --config _scripts/webpack.renderer.config.js",
    "pack:workers": "webpack --mode=production --hide-modules --config _scripts/webpack.workers.config.js",
    "postinstall": "electron-rebuild",
    "prettier": "prettier --write \"{src,_scripts}/**/*.{js,ts,vue}\"",
    "rebuild:electron": "run-s electron-builder-install electron-rebuild",
    "rebuild:node": "npm rebuild",
    "release": "run-s test build",
    "test": "run-s test:unit test:e2e",
    "test:coverage": "run-s rebuild:node pack:workers jest:coverage",
    "test:e2e": "run-s rebuild:electron pack jest:e2e",
    "test:unit": "run-s rebuild:node pack:workers jest:unit",
    "test:watch": "run-s rebuild:node pack:workers jest:watch",
    "typesync": "typesync --ignoredeps=dev",
    "webpack:analyze:main": "cross-env NODE_ENV=production webpack --config _scripts/webpack.main.config.js  --profile --json > .tmp/webpack-stats.json && whybundled .tmp/webpack-stats.json",
    "webpack:analyze:renderer": "cross-env NODE_ENV=production webpack --config _scripts/webpack.renderer.config.js  --profile --json > .tmp/webpack-stats.json && whybundled .tmp/webpack-stats.json",
    "webpack:analyze:workers": "cross-env NODE_ENV=production webpack --config _scripts/webpack.workers.config.js  --profile --json > .tmp/webpack-stats.json && whybundled .tmp/webpack-stats.json"
  },
  "sideEffects": false,
  "version": "0.0.1"
