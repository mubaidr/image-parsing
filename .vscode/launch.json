{
  "configurations": [
    {
      "args": ["${relativeFile}"],
      "cwd": "${workspaceRoot}",
      "name": "Current TS File",
      "protocol": "inspector",
      "request": "launch",
      "runtimeArgs": ["--nolazy", "-r", "ts-node/register"],
      "sourceMaps": true,
      "type": "node"
    },
    {
      "cwd": "${workspaceFolder}",
      "name": "Electron: Main",
      "port": 9222,
      "protocol": "inspector",
      "request": "attach",
      "sourceMaps": true,
      "type": "node"
    },
    {
      "name": "Electron: Renderer",
      "port": 9223,
      "request": "attach",
      "sourceMaps": true,
      "type": "chrome",
      "webRoot": "${workspaceFolder}"
    }
  ],
  "version": "0.2.0"
}
