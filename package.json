{
  "author": "Muhammad Ubaid Raza <mubaidr@gmail.com>",
  "bugs": {
    "url": "https://github.com/mubaidr/image-parsing/issues"
  },
  "build": {
    "appId": "com.mubaidr.image-parsing",
    "asar": false,
    "directories": {
      "output": "build/"
    },
    "dmg": {
      "contents": [
        {
          "path": "/Applications",
          "type": "link",
          "x": 410,
          "y": 230
        },
        {
          "type": "file",
          "x": 130,
          "y": 230
        }
      ],
      "window": {
        "height": 380,
        "width": 540
      }
    },
    "files": [
      "dist/**/*",
      "_icons/icon.*"
    ],
    "linux": {
      "icon": "_icons/icon.png",
      "target": [
        "deb",
        "snap",
        "AppImage"
      ]
    },
    "mac": {
      "category": "public.app-category.utilities",
      "icon": "_icons/icon.icns",
      "target": [
        "dmg",
        "zip"
      ],
      "type": "distribution"
    },
    "npmRebuild": false,
    "nsis": {
      "allowToChangeInstallationDirectory": true,
      "oneClick": false
    },
    "productName": "Image Parsing",
    "win": {
      "icon": "_icons/icon.ico",
      "target": [
        "nsis",
        "zip",
        "portable"
      ]
    }
  },
  "dependencies": {
    "brain.js": "^1.6.1",
    "bulma-pro": "^0.1.4",
    "cheerio": "1.0.0-rc.2",
    "electron-settings": "^3.2.0",
    "fast-glob": "^2.2.6",
    "javascript-barcode-reader": "^0.4.11",
    "physical-cpu-count": "^2.0.0",
    "sharp": "^0.21.3",
    "vue": "^2.6.6",
    "vue-router": "^3.0.2"
  },
  "description": "An electron-vue project",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "devtron": "^1.4.0",
    "electron": "4.0.4",
    "electron-builder": "^20.38.5",
    "electron-debug": "^2.1.0",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-vue": "^5.2.1",
    "file-loader": "^3.0.1",
    "glob": "^7.1.3",
    "html-webpack-plugin": "^3.2.0",
    "inject-loader": "^4.0.1",
    "jest": "^24.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-abi": "^2.7.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.11.0",
    "prettier": "^1.16.4",
    "purgecss-webpack-plugin": "^1.4.0",
    "regenerator-runtime": "^0.13.1",
    "sass-loader": "^7.1.0",
    "script-ext-html-webpack-plugin": "^2.1.3",
    "style-loader": "^0.23.1",
    "tree-kill": "1.2.1",
    "url-loader": "^1.1.2",
    "vue-devtools": "^5.0.0-beta.1",
    "vue-eslint-parser": "^6.0.2",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.6.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.6",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14",
    "webpack-hot-middleware": "^2.24.3"
  },
  "jest": {
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/utilities/**/*.js",
      "!src/utilities/tmp/"
    ],
    "coverageDirectory": "coverage",
    "testPathIgnorePatterns": [
      "__tests__/.eslintrc.js"
    ]
  },
  "license": "MIT",
  "main": "dist/main.js",
  "name": "image-parsing",
  "productName": "Image Parsing",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mubaidr/image-parsing.git"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production npm run pack && electron-builder",
    "build:dir": "cross-env NODE_ENV=production npm run pack && electron-builder --dir",
    "build:linux": "npm run pack && electron-builder --linux",
    "build:mac": "npm run pack && electron-builder --mac",
    "build:win": "npm run pack && electron-builder --win",
    "dev": "node _scripts/dev-runner.js",
    "lint": "eslint --ext .js,.vue --fix src __tests__ && prettier --write src/**/*.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "webpack --hide-modules --config _scripts/webpack.main.config.js",
    "pack:renderer": "webpack --hide-modules --config _scripts/webpack.renderer.config.js",
    "postinstall": "node node_modules/sharp/install/dll-copy.js",
    "release": "build",
    "test": "jest"
  },
  "version": "0.0.1"
}
