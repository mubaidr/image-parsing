{
  "author": "Muhammad Ubaid Raza <mubaidr@gmail.com>",
  "bugs": {
    "url": "https://github.com/mubaidr/image-parsing/issues"
  },
  "build": {
    "appId": "org.mubaidr.image-parsing",
    "directories": {
      "output": "build"
    },
    "dmg": {
      "contents": [
        {
          "path": "/Applications",
          "type": "link",
          "x": 410,
          "y": 150
        },
        {
          "type": "file",
          "x": 130,
          "y": 150
        }
      ]
    },
    "files": [
      "dist/electron/**/*"
    ],
    "linux": {
      "icon": "build/icons"
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "productName": "image-parsing",
    "win": {
      "icon": "build/icons/icon.ico"
    }
  },
  "dependencies": {
    "brain.js": "^1.6.0",
    "bulma-addons": "^0.1.12",
    "bulma-pro": "^0.1.4",
    "cheerio": "1.0.0-rc.2",
    "downloadjs": "^1.4.7",
    "electron-store": "^2.0.0",
    "fast-glob": "^2.2.4",
    "javascript-barcode-reader": "^0.4.7",
    "pdfmake": "^0.1.39",
    "physical-cpu-count": "^2.0.0",
    "sharp": "^0.21.1",
    "vue": "^2.5.21",
    "vue-router": "^3.0.2",
    "vuex": "^3.0.1"
  },
  "description": "An electron-vue project",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.0.1",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "3.0.13",
    "electron-builder": "^20.38.4",
    "electron-debug": "^2.0.0",
    "eslint": "^5.11.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-vue": "^5.0.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "inject-loader": "^4.0.1",
    "jest": "^23.6.0",
    "node-abi": "^2.5.0",
    "node-loader": "^0.6.0",
    "node-sass": "^4.11.0",
    "prettier": "^1.15.3",
    "regenerator-runtime": "^0.13.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "tree-kill": "1.2.1",
    "url-loader": "^1.1.2",
    "vue-devtools": "^5.0.0-beta.1",
    "vue-eslint-parser": "^4.0.3",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.4.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.21",
    "webpack": "^4.28.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-hot-middleware": "^2.24.3"
  },
  "jest": {
    "bail": true,
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/utilities/**/*.js",
      "!src/utilities/tmp/"
    ],
    "coverageDirectory": "__coverage__",
    "globalSetup": "",
    "globals": {},
    "setupFiles": [],
    "testPathIgnorePatterns": [
      "__tests__/.eslintrc.js"
    ]
  },
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "name": "image-parsing",
  "productName": "Image Parsing",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mubaidr/image-parsing.git"
  },
  "scripts": {
    "build": "yarn run pack && electron-builder",
    "build:dir": "yarn run pack && electron-builder --dir",
    "build:linux": "yarn run pack && electron-builder --linux",
    "build:mac": "yarn run pack && electron-builder --mac",
    "build:win": "yarn run pack && electron-builder --win",
    "build:win-app": "yarn run pack && electron-builder",
    "clean": "cross-env BUILD_TARGET=clean node vue-electron/build.js",
    "dev": "cross-env NODE_ENV=development node vue-electron/dev-runner.js",
    "lint": "eslint --ext .js,.vue --fix src __tests__ && prettier --write ./src/**/*.js",
    "pack": "cross-env NODE_ENV=production yarn run pack:main && yarn run pack:renderer",
    "pack:main": "webpack --mode production --hide-modules --config vue-electron/webpack.main.config.js",
    "pack:renderer": "webpack --mode production --hide-modules --config vue-electron/webpack.renderer.config.js",
    "postinstall": "electron-builder install-app-deps",
    "release": "build",
    "test": "jest --no-color",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "version": "0.0.1"
}
