{
  "author": "Muhammad Ubaid Raza <mubaidr@gmail.com>",
  "bugs": {
    "url": "https://github.com/mubaidr/image-parsing/issues"
  },
  "build": {
    "appId": "org.mubaidr.image-parsing",
    "directories": {
      "output": "build"
    },
    "dmg": {
      "contents": [
        {
          "path": "/Applications",
          "type": "link",
          "x": 410,
          "y": 150
        },
        {
          "type": "file",
          "x": 130,
          "y": 150
        }
      ]
    },
    "files": [
      "dist/electron/**/*"
    ],
    "linux": {
      "icon": "build/icons"
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "productName": "image-parsing",
    "win": {
      "icon": "build/icons/icon.ico"
    }
  },
  "dependencies": {
    "brain.js": "^1.2.8",
    "bulma-addons": "^0.1.12",
    "bulma-fluent": "^0.3.11",
    "cheerio": "1.0.0-rc.2",
    "downloadjs": "^1.4.7",
    "electron-store": "^2.0.0",
    "fast-glob": "^2.2.2",
    "javascript-barcode-reader": "^0.3.0",
    "pdfmake": "^0.1.38",
    "physical-cpu-count": "^2.0.0",
    "sharp": "^0.20.8",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "description": "An electron-vue project",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron-builder": "^20.28.4",
    "electron-debug": "^2.0.0",
    "electron-rebuild": "^1.8.2",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-vue": "^4.7.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "inject-loader": "^4.0.1",
    "jest": "^23.6.0",
    "node-loader": "^0.6.0",
    "node-sass": "^4.9.3",
    "prettier": "^1.14.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "url-loader": "^1.1.1",
    "vue-devtools": "^4.1.4",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.4.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.18.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "3.1.8",
    "webpack-hot-middleware": "^2.24.0"
  },
  "jest": {
    "bail": true,
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/utilities/**/*.js",
      "!src/utilities/tmp/"
    ],
    "coverageDirectory": "__coverage__",
    "globalSetup": "",
    "globals": {},
    "setupFiles": [],
    "testPathIgnorePatterns": [
      "__tests__/.eslintrc.js"
    ]
  },
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "name": "image-parsing",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mubaidr/image-parsing.git"
  },
  "scripts": {
    "build": "npm run pack && electron-builder",
    "build:dir": "npm run pack && electron-builder --dir",
    "build:linux": "npm run pack && electron-builder --linux",
    "build:mac": "npm run pack && electron-builder --mac",
    "build:win": "npm run pack && electron-builder --win",
    "build:win-app": "npm run pack && electron-builder",
    "clean": "cross-env BUILD_TARGET=clean node vue-electron/build.js",
    "dev": "cross-env NODE_ENV=development node vue-electron/dev-runner.js",
    "electron-rebuild": "./node_modules/.bin/electron-rebuild",
    "lint": "eslint --ext .js,.vue src test",
    "lint:fix": "eslint --ext .js,.vue --fix src test",
    "pack": "cross-env NODE_ENV=production npm run pack:main && npm run pack:renderer",
    "pack:main": "webpack --mode production --hide-modules --config vue-electron/webpack.main.config.js",
    "pack:renderer": "webpack --mode production --hide-modules --config vue-electron/webpack.renderer.config.js",
    "postinstall": "npm rebuild && electron-builder install-app-deps && npm run electron-rebuild",
    "prettier": "prettier --write ./src/**/*.js && prettier --write ./src/**/*.vue ",
    "release": "build",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "version": "0.0.1"
}
